<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../lib/internal/Magento/Framework/Acl/etc/acl.xsd">
<acl>
   <resources>
      <resource id="Magento_Backend::admin">
         <resource id="FME_Extensions::fmeextensions" title="FME Extensions" sortOrder="70">
            <resource id="FME_Productvideos::fmeextensions_productvideos" title="Product Videos" sortOrder="80">
               <resource id="FME_Productvideos::fmeextensions_productvideos_items" title="Manage Videos" sortOrder="80"/>
            </resource>
         </resource>
         <resource id="Magento_Backend::stores">
            <resource id="Magento_Backend::stores_settings">
              <resource id="Magento_Config::config">
                  <resource id="FME_Productvideos::fmeextensions_productvideos_config" title="Product Videos Section"/>
               </resource>
            </resource>
         </resource>
      </resource>
   </resources>
</acl>
</config>